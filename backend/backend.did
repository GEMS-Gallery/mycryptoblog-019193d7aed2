type Result_1 = 
 variant {
   err: text;
   ok: nat;
 };
type Result = 
 variant {
   err: text;
   ok;
 };
type Post = 
 record {
   content: text;
   id: nat;
   imageUrl: opt text;
   timestamp: int;
   title: text;
 };
service : {
  createPost: (text, text, opt text) -> (Result_1);
  editPost: (nat, text, text, opt text) -> (Result);
  getPosts: () -> (vec Post) query;
}
